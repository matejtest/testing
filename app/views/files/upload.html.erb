<div class="container">
	<div class="row">
		<div class="col-sm-6 col-md-4 col-md-offset-4">
				<%= form_tag import_data_path(), :method => :post, :id => 'upload_form', :multipart => true, :class => 'form-signin' do %>
					<%= file_field_tag "attachment"  %>
					<%= button_tag( "Upload file", :class => "btn btn-lg btn-primary btn-block" ) %>
				<% end %>
		</div>
	</div>
</div>